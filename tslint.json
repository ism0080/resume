{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier", "tslint-plugin-prettier"],
  "import-name": false,
  "ban": [
    true,
    "eval",
    { "name": "$", "message": "please don't jquery is banned" },
    { "name": "JQuery", "message": "please don't jquery is banned" },
    { "name": "React", "message": "the case for this is wrong" }
  ],
  "jsRules": {},
  "rules": { "prettier": true, "interface-name": [false, "never-prefix"] },
  "rulesDirectory": [],
  "semicolon": false,
  "max-func-body-length": [true, 160],
  "max-file-line-count": [true, 325],
  "prefer-dry-conditionals": true,
  "no-with-statement": true,
  "no-this-assignment": true,
  "no-unexpected-multiline": true,
  "no-any": true,
  "prefer-for-of": true,
  "arrow-return-shorthand": [true, "multiline"],
  "promise-function-async": true,
  "only-arrow-functions": true,
  "interface-name": [true, "never-prefix"],
  "no-empty-interface": true,
  "no-internal-module": true,
  "no-magic-numbers": true,
  "no-unnecessary-type-assertion": true,
  "no-increment-decrement": false,
  "ban-comma-operator": true,
  "newline-before-return": true,
  "tsr-detect-possible-timing-attacks": false,
  "no-duplicate-super": true,
  "no-duplicate-switch-case": true,
  "no-duplicate-variable": true,
  "no-commented-code": false,
  "no-small-switch": false,
  "prefer-object-spread": true,
  "no-namespace": true,
  "object-literal-sort-keys": false,
  "tsr-detect-unsafe-regexp": false,
  "jsx-boolean-value": false,
  "radix": false,
  "file-name-casing": [true, "kebab-case"],
  "no-non-null-assertion": false,
  "array-type": false,
  "max-union-size": false,
  "react-hooks-nesting": true
}
